{
  "name": "sentry",
  "version": "2.0.0",
  "description": "Moderate with ease. A Discord bot focused on easy moderation.",
  "main": "index.js",
  "repository": "https://github.com/Anidox/Sentry.git",
  "author": "Anidox <24899167+Anidox@users.noreply.github.com>",
  "license": "GPL-3.0-or-later",
  "private": false,
  "scripts": {
    "lint": "yarn run lint:src && yarn run lint:web",
    "lint:src": "eslint --fix src",
    "lint:web": "eslint --fix -c web/.eslintrc.json --ignore-path web/.eslintignore web",
    "build": "yarn run build:src && yarn run build:web",
    "build:src": "tsc",
    "build:web": "next build web",
    "start": "yarn run build && node dist/index.js",
    "dev": "yarn run lint && yarn run start"
  },
  "devDependencies": {
    "@types/mongoose": "^5.10.3",
    "@types/react": "^17.0.3",
    "@types/signale": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-plugin-react": "^7.22.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@typegoose/typegoose": "^7.6.0",
    "detritus-client": "^0.13.0",
    "dotenv": "^8.2.0",
    "mongoose": "5.10.18",
    "next": "^10.0.8",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "signale": "^1.4.0"
  }
}
